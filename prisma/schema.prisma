generator client {
  provider = "prisma-client-js"
}

datasource db {
  provider = "postgresql"
  url      = env("DATABASE_URL")
}

model note {
  id         BigInt    @id(map: "notes_pkey") @default(autoincrement())
  title      String
  content    String?
  created_at DateTime? @default(now()) @db.Timestamptz(6)
}
